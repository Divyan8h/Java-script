        alert("Your BMI is "+ interpretation + ", so you have a normal weight.");
    }
    else if (interpretation > 24.9){
        alert("Your BMI is "+ interpretation + ", so you are overweight.");
    }
    return interpretation;
}

bmiCalculator(60,2);
